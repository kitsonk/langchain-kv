{
  "name": "@kitsonk/langchain-kv",
  "version": "0.2.0",
  "exports": {
    ".": "./mod.ts",
    "./cache": "./cache.ts",
    "./chat-history": "./chat_history.ts",
    "./checkpoint": "./checkpoint.ts",
    "./record-manager": "./record_manager.ts",
    "./store": "./store.ts"
  },
  "imports": {
    "@kitsonk/kv-toolbox": "jsr:@kitsonk/kv-toolbox@^0.30.0",
    "@langchain/core": "npm:@langchain/core@^0.3.55",
    "@langchain/langgraph-checkpoint": "npm:@langchain/langgraph-checkpoint@^0.1.0",
    "zod": "npm:zod@^4.0.14"
  },
  "publish": {
    "exclude": [
      "_*.ts",
      "*.test.ts",
      ".github",
      ".vscode"
    ]
  },
  "fmt": {
    "lineWidth": 120
  },
  "tasks": {
    "check": "deno fmt --check && deno lint && deno publish --dry-run --allow-dirty && deno check --doc *.ts",
    "coverage": "deno coverage --lcov --output=cov.lcov ./cov",
    "test": "deno test --allow-read --allow-write --allow-env --unstable-kv --parallel",
    "test:ci": "deno test --allow-read --allow-write --allow-env --unstable-kv --junit-path=junit.xml --coverage=./cov --parallel"
  },
  "lock": false,
  "nodeModulesDir": "auto"
}
